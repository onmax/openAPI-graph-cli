{
  "name": "openapi-graph-cli",
  "version": "1.0.0",
  "description": "A cli to manage analyze API projects defined by OpenAPIv3 specification.",
  "main": "./lib/index.js",
  "bin": {
    "apig": "lib/index.js"
  },
  "keywords": [
    "openapi3",
    "graph-structures",
    "ts-library",
    "cli",
    "openapi3-cli"
  ],
  "homepage": "https://github.com/onmax/openapi-graph-cli",
  "bugs": "https://github.com/onmax/openapi-graph-cli/issues",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts analyze ../",
    "test": "jest",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "create": "npm run build && npm run test",
    "local": "sudo npm i -g && apig",
    "lint": "tslint -p tsconfig.json",
    "debug": "node --debug-brk --inspect ./node_modules/jest/bin/jest -i",
    "release": "release-it"
  },
  "author": "onmax",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "repository": {
    "url": "git://github.com/onmax/open-api-graph-cli.git"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.37",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "openapi-types": "^7.2.3",
    "prettier": "^2.2.1",
    "release-it": "^14.6.1",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "commander": "^7.2.0",
    "openapi-graph-core": "file:../openapi-graph-core",
    "openapi-graph-types": "file:../openapi-graph-types"
  }
}
